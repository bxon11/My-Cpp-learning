#include <iostream>
#include <cmath>


using namespace std;

int main() {

    int num1, num2, num3,num4, num5;
    int odd= 0, even = 0;
    cout << "Enter five integers: ";
    cin >> num1 >> num2 >> num3 >> num4 >> num5;

    if(num1 % 2 == 1){odd ++;}
    if(num2 % 2 == 1){odd ++;}
    if(num3 % 2 == 1){odd ++;}
    if(num4 % 2 == 1){odd ++;}
    if(num5 % 2 == 1){odd ++;}
    if(num1 % 2 == 0){even ++;}
    if(num2 % 2 == 0){even ++;}
    if(num3 % 2 == 0){even ++;}
    if(num4 % 2 == 0){even ++;}
    if(num5 % 2 == 0){even ++;}
        cout << "Odd numbers: " << odd << endl;
    cout << "Even numbers: " << even << endl;



    return 0;
}
