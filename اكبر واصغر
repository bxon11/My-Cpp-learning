#include <iostream>
using namespace std;
int main(){
    
int numbers[5];
for (int i = 0; i < 5; i++)
{
    cout << "Enter number " << i + 1 << " : ";
    cin >> numbers[i];
}


int max = numbers[0];
int min = numbers[0];

for(int i = 0; i < 5; i++){
if(numbers[i] > max) max = numbers[i];
if(numbers[i] < min ) min = numbers[i];
    
}

cout << "Maximum number is: " << max << endl;
cout <<"Minmum number is: "<< min ;

    return 0;
}
